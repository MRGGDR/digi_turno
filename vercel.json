{
  "version": 2,
  "builds": [
    {
      "src": "src/**",
      "use": "@vercel/static"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/pages/login.html",
      "permanent": false
    },
    {
      "source": "/index.html",
      "destination": "/pages/login.html",
      "permanent": false
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/$1"
    }
  ]
}
